{
    "name": "g4m4",
    "homepage": "https://github.com/iZemil/one-page",
    "version": "1.0.0",
    "main": "src/index.jsx",
    "scripts": {
        "start": "npm run dev:local-backend",
        "dev": "webpack-dev-server --mode=development --open",
        "dev:local-backend": "cross-env BACKEND=LOCAL webpack-dev-server --mode=development --open",
        "build": "webpack --mode=production",
        "build:dev": "webpack --mode=development",
        "format": "npm run format:js && npm run format:css",
        "format:js": "prettier --write \"src/**/*.{js,jsx}\"",
        "format:css": "stylus-supremacy format \"src/**/*.styl\" --replace --options .stylussupremacy",
        "lint": "eslint src --ext .js,.jsx",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "express": "pm2 start ./express-server.js --name express-g4m4"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "npm run lint --fix",
            "prettier --write",
            "git add"
        ],
        "src/**/*.{css,less}": [
            "prettier --write",
            "git add"
        ],
        "src/**/*.styl": [
            "stylus-supremacy format --replace --options .stylussupremacy",
            "git add"
        ]
    },
    "dependencies": {
        "classnames": "2.2.6",
        "decko": "1.2.0",
        "express": "^4.17.1",
        "mobx": "5.9.0",
        "mobx-react": "5.4.3",
        "moment": "^2.24.0",
        "prop-types": "15.6.2",
        "react": "16.8",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "16.8",
        "react-modal": "^3.11.2",
        "react-router-dom": "4.3.1",
        "react-skylight": "^0.5.1",
        "react-slick": "^0.24.0",
        "slick-carousel": "^1.8.1"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-proposal-class-properties": "7.2.3",
        "@babel/plugin-proposal-decorators": "7.2.3",
        "@babel/plugin-proposal-export-default-from": "7.2.0",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-syntax-import-meta": "7.2.0",
        "@babel/plugin-transform-runtime": "7.2.0",
        "@babel/preset-env": "7.2.3",
        "@babel/preset-react": "7.0.0",
        "@babel/runtime": "7.2.0",
        "@commitlint/cli": "7.3.2",
        "@commitlint/config-conventional": "7.3.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.5",
        "babel-plugin-import": "1.11.0",
        "clean-webpack-plugin": "1.0.0",
        "commitlint": "7.3.2",
        "copy-webpack-plugin": "5.1.1",
        "cross-env": "^5.2.0",
        "css-loader": "1.0.1",
        "cssnano": "4.1.8",
        "error-overlay-webpack-plugin": "0.1.6",
        "eslint": "5.12.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "3.5.0",
        "eslint-import-resolver-webpack": "0.11.0",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-react": "7.12.4",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "1.1.11",
        "gh-pages": "^2.0.1",
